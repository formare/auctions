Cell[CellGroupData[{

Cell["Second-price auction", "Subsection",
 CellChangeTimes->{{3.551871500427945*^9, 3.551871503480475*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "A ",
 StyleBox["second-price auction",
  FontWeight->"Bold"],
 " is an auction whose outcome satisfies the following conditions:\n",
 StyleBox["1. One of the participants with the highest bids wins. (We do not \
formalise the random selection of one distinct participants from the set of \
highest bidders, in case there is more than one.)\n2. The price that the \
winner pays is the maximum bid that remains after removing the winner's own \
bid from the vector of bids.\n3. The losers do not pay anything.", 
  "ItemNumbered"]
}], "Text",
 CellGroupingRules->{GroupTogetherGrouping, 10012.},
 CellChangeTimes->{{3.551715488804222*^9, 3.551715532789406*^9}, {
   3.551715580876706*^9, 3.551715773989786*^9}, {3.551716647404941*^9, 
   3.551716702795518*^9}, {3.5517167386272707`*^9, 3.551716755939196*^9}, 
   3.551716982041741*^9, {3.5517346469960823`*^9, 3.551734651749243*^9}}],

Cell[BoxData[
 RowBox[{"Definition", "[", 
  RowBox[{"\"\<SecondPriceAuction\>\"", ",", " ", 
   RowBox[{"any", "[", 
    RowBox[{"a", ",", "n"}], "]"}], ",", " ", 
   RowBox[{"with", "[", 
    RowBox[{"auction", "[", 
     RowBox[{"a", ",", "n"}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"SecondPriceAuction", "[", 
     RowBox[{"a", ",", "n"}], "]"}], " ", ":", 
    RowBox[{"\[DoubleLeftRightArrow]", 
     RowBox[{
      UnderscriptBox[
       UnderscriptBox["\[ForAll]", 
        RowBox[{"b", ",", "x", ",", "p"}]], 
       RowBox[{
        RowBox[{"a", "[", "b", "]"}], "=", 
        RowBox[{"{", 
         RowBox[{"x", ",", "p"}], "}"}]}]], " ", 
      RowBox[{
       UnderscriptBox["\[Exists]", 
        RowBox[{"i", "\[Element]", 
         RowBox[{"M", "[", 
          RowBox[{"n", ",", "b"}], "]"}]}]], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"x", "[", "i", "]"}], "=", " ", "1"}], ")"}], " ", 
         "\[And]", " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"p", "[", "i", "]"}], " ", "=", " ", 
           RowBox[{"maximalExcept", "[", 
            RowBox[{"n", ",", "b", ",", "i"}], "]"}]}], ")"}], " ", "\[And]", 
         " ", 
         RowBox[{"(", 
          RowBox[{
           UnderscriptBox[
            UnderscriptBox["\[ForAll]", 
             RowBox[{
              RowBox[{"j", "=", "1"}], ",", "\[Ellipsis]", ",", "n"}]], 
            RowBox[{"j", "\[NotEqual]", "i"}]], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"x", "[", "j", "]"}], " ", "=", " ", "0"}], " ", ")"}],
              "\[And]", " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"p", "[", "j", "]"}], " ", "=", " ", "0"}], ")"}]}], 
            ")"}]}], ")"}]}], ")"}]}]}]}]}]}], "]"}]], "Input",
 CellGroupingRules->{GroupTogetherGrouping, 10012.},
 InitializationCell->True,
 CellChangeTimes->{{3.548933896786576*^9, 3.548933910764289*^9}, {
