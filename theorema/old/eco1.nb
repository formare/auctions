    RowBox[{"any", "[", 
     RowBox[{"n", ",", "x", ",", "y"}], "]"}], ",", "\[IndentingNewLine]", 
    GridBox[{
      {
       RowBox[{
        RowBox[{"W", "[", 
         RowBox[{"n", ",", "x", ",", "y"}], "]"}], " ", ":=", " ", 
        RowBox[{"{", 
         RowBox[{"i", 
          UnderscriptBox["\[VerticalSeparator]", 
           RowBox[{"i", "\[Element]", 
            RowBox[{"I", "[", "n", "]"}]}]], 
          RowBox[{
           SubscriptBox["y", "i"], ">", 
           SubscriptBox["x", "i"]}]}], "}"}]}], "\"\<W\>\""},
      {
       RowBox[{
        RowBox[{"L", "[", 
         RowBox[{"n", ",", "x", ",", "y"}], "]"}], " ", ":=", " ", 
        RowBox[{"{", 
         RowBox[{"i", 
          UnderscriptBox["\[VerticalSeparator]", 
           RowBox[{"i", "\[Element]", 
            RowBox[{"I", "[", "n", "]"}]}]], 
