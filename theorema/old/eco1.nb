Cell[CellGroupData[{

Cell["\<\
Pillage games have at least two agents; we formalize this as an additional \
condition on any number n. This check is intended to be made whenever a set \
of agents occurs.\
\>", "Text",
 CellGroupingRules->{GroupTogetherGrouping, 10000.},
 CellChangeTimes->{{3.5518701356782722`*^9, 3.551870178677699*^9}, {
  3.551870379011661*^9, 3.551870395981244*^9}, {3.552153760543213*^9, 
  3.55215379808829*^9}}],

Cell[BoxData[
 RowBox[{"Definition", "[", 
  RowBox[{"\"\<appropriateLength\>\"", ",", " ", 
   RowBox[{"any", "[", "n", "]"}], ",", " ", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"appropriateLength", "[", "n", "]"}], ":", 
    RowBox[{"\[DoubleLeftRightArrow]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"n", "\[Element]", "\[DoubleStruckCapitalN]"}], "\[And]", 
       RowBox[{"n", "\[GreaterEqual]", "2"}]}], ")"}]}]}]}], "]"}]], "Input",
 CellGroupingRules->{GroupTogetherGrouping, 10000.},
 InitializationCell->True,
 CellChangeTimes->{{3.507290953693247*^9, 3.507290984710874*^9}, 
   3.5518701682173758`*^9, {3.5521537229708033`*^9, 3.5521537229769573`*^9}},
 CellTags->"Definition (appropriateLength)"]
}, Open  ]],

