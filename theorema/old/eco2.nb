Cell["Dominion", "Subsection",
 CellChangeTimes->{{3.5518911300547028`*^9, 3.551891130942492*^9}}],

Cell[TextData[{
 "The ",
 StyleBox["dominion",
  FontWeight->"Bold"],
 " of a set of allocations Y is the set of all allocations x that are \
dominated by an allocation in Y."
}], "Text",
 CellChangeTimes->{{3.551890751737215*^9, 3.551890771625442*^9}, {
   3.551890827322184*^9, 3.551890851264608*^9}, 3.551891200758857*^9}],

Cell[BoxData[
 RowBox[{"Definition", "[", 
  RowBox[{"\"\<dominion\>\"", ",", " ", 
   RowBox[{"any", "[", 
    RowBox[{"\[Pi]", ",", "n", ",", "Y"}], "]"}], ",", "\[IndentingNewLine]", 
   
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{"Y", ",", "\[Pi]", ",", "n"}], "]"}], ":=", 
    RowBox[{"{", 
     RowBox[{"x", 
      UnderscriptBox["\[VerticalSeparator]", 
       RowBox[{
        SubscriptBox["allocation", "n"], "[", "x", "]"}]], 
      RowBox[{
       UnderscriptBox["\[Exists]", "y"], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"y", "\[Element]", "Y"}], "\[And]", 
         RowBox[{"dominates", "[", 
          RowBox[{"y", ",", "x", ",", "\[Pi]", ",", "n"}], "]"}]}], ")"}]}]}],
      "}"}]}]}], "]"}]], "Input",
